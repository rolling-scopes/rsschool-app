{
  "name": "nestjs",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "Mozilla Public License 2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "compile": "tsc --noEmit",
    "start": "nest start --watch | pino-pretty -i time,remoteAddress,req,reqId",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "openapi": "npm run openapi:spec && npm run openapi:sdk && npm run openapi:format",
    "openapi:spec": "ts-node --project tsconfig.json ./src/openapi-spec.ts",
    "openapi:format": "prettier --write src/spec.json",
    "openapi:sdk": "npx openapi-generator-cli generate -i ./src/spec.json -g typescript-axios -o ../client/src/api --additional-properties=supportsES6=true"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.9.0",
    "@aws-sdk/client-s3": "3.749.0",
    "@nestjs/axios": "4.0.0",
    "@nestjs/cache-manager": "3.0.0",
    "@nestjs/common": "11.0.9",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.9",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.9",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/swagger": "11.0.3",
    "@nestjs/typeorm": "11.0.0",
    "@sentry/node": "8.47.0",
    "@vendia/serverless-express": "4.10.4",
    "cache-manager": "6.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "express": "5.0.1",
    "handlebars": "4.7.8",
    "ical-generator": "5.0.0",
    "json2csv": "5.0.7",
    "jsonwebtoken": "9.0.2",
    "nanoid": "3.3.8",
    "nestjs-pino": "4.3.0",
    "openai": "4.19.0",
    "passport": "0.6.0",
    "passport-custom": "1.1.1",
    "passport-github2": "0.1.12",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.3",
    "pino-http": "8.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.9",
    "@openapitools/openapi-generator-cli": "2.16.3",
    "@types/cookie-parser": "1.4.6",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@types/json2csv": "5.0.7",
    "@types/jsonwebtoken": "9.0.5",
    "@types/passport-github2": "1.2.9",
    "@types/passport-http": "0.3.11",
    "@types/passport-jwt": "3.0.13",
    "@types/supertest": "2.0.16",
    "jest-junit": "16.0.0",
    "pino-pretty": "10.2.3",
    "supertest": "6.3.3",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".*\\.spec\\.ts$",
      "test\\.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@entities(.*)$": "<rootDir>/../../server/src/models/$1",
      "^src/(.*)$": "<rootDir>/$1"
    },
    "clearMocks": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-nestjs.xml"
  }
}
