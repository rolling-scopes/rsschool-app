{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start --watch | pino-pretty -i time,remoteAddress,req,reqId",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "openapi": "npm run openapi:spec && npm run openapi:sdk && npm run openapi:format",
    "openapi:spec": "ts-node --project tsconfig.json ./src/openapi-spec.ts",
    "openapi:format": "prettier --write src/spec.json",
    "openapi:sdk": "npx openapi-generator-cli generate -i ./src/spec.json -g typescript-axios -o ../client/src/api --additional-properties=supportsES6=true"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.8.6",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/mapped-types": "1.1.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@vendia/serverless-express": "4.10.1",
    "cache-manager": "4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "ical-generator": "3.5.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "nanoid": "3.3.4",
    "nestjs-pino": "3.1.1",
    "passport": "0.6.0",
    "passport-custom": "1.1.1",
    "passport-github2": "0.1.12",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "pg": "8.8.0",
    "pino-http": "8.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.11",
    "@openapitools/openapi-generator-cli": "2.5.1",
    "@types/cache-manager": "4.0.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "29.0.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.184",
    "@types/node": "16.11.52",
    "@types/passport-github2": "1.2.5",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.3",
    "jest-junit": "14.0.1",
    "pino-pretty": "9.0.1",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "29.0.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".*\\.spec\\.ts$",
      "test\\.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@entities(.*)$": "<rootDir>/../../server/src/models/$1",
      "^src/(.*)$": "<rootDir>/$1"
    },
    "clearMocks": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-nestjs.xml"
  }
}
