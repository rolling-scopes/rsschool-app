{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "openapi": "npm run openapi:spec && npm run openapi:sdk",
    "openapi:spec": "ts-node --project tsconfig.json ./src/openapi-spec.ts",
    "openapi:sdk": "npx openapi-generator-cli generate -i ./src/spec.json -g typescript-axios -o ../client/src/api --additional-properties=supportsES6=true"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.8.6",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.3",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.3",
    "@nestjs/mapped-types": "1.0.1",
    "@nestjs/passport": "8.2.1",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/platform-express": "8.4.3",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.3",
    "@vendia/serverless-express": "4.5.3",
    "cache-manager": "3.6.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.0",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "nanoid": "3.3.1",
    "nestjs-pino": "2.5.0",
    "passport": "0.5.2",
    "passport-custom": "1.1.1",
    "passport-github2": "0.1.12",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "pino-http": "6.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.3.0",
    "typeorm": "0.2.43"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.3",
    "@openapitools/openapi-generator-cli": "2.5.1",
    "@types/cache-manager": "3.4.2",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.25",
    "@types/passport-github2": "1.2.5",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "jest-junit": "13.0.0",
    "pino-pretty": "7.6.0",
    "prettier": "2.6.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@entities(.*)$": "<rootDir>/../../server/src/models/$1"
    }
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-nestjs.xml"
  }
}
