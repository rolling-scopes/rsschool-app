{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "tsnd --ignore-watch node_modules --inspect --notify false -r dotenv/config -r tsconfig-paths/register -- ./src/index.ts | pino-pretty -i time,hostname,pid,host,method,remoteAddress",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "typeorm": "npx ts-node -r dotenv/config ./node_modules/typeorm/cli.js -f ./src/ormconfig.ts"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.8.0",
    "@koa/cors": "3.0.0",
    "@koa/router": "8.0.8",
    "aws-sdk": "2.1022.0",
    "axios": "0.24.0",
    "http-status-codes": "2.1.4",
    "ics": "2.31.0",
    "json2csv": "5.0.6",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-basic-auth": "4.0.0",
    "koa-bodyparser": "4.2.1",
    "koa-jwt": "3.6.0",
    "koa-passport": "4.1.3",
    "koa-session": "5.13.1",
    "koa-static": "5.0.0",
    "koa-typeorm-pagination": "1.0.1",
    "lodash": "4.17.21",
    "luxon": "2.0.2",
    "moment-timezone": "0.5.33",
    "node-cache": "5.1.2",
    "node-schedule": "2.0.0",
    "octokit": "1.7.0",
    "passport-github2": "0.1.12",
    "pg": "8.7.1",
    "pino-multi-stream": "4.2.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.38"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/json2csv": "5.0.3",
    "@types/jsonwebtoken": "8.5.5",
    "@types/koa": "2.11.3",
    "@types/koa__cors": "2.2.3",
    "@types/koa__router": "8.0.2",
    "@types/koa-basic-auth": "2.0.4",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-passport": "4.0.2",
    "@types/koa-session": "5.10.2",
    "@types/koa-static": "4.0.2",
    "@types/lodash": "4.14.176",
    "@types/luxon": "2.0.5",
    "@types/node": "16.11.6",
    "@types/node-schedule": "1.3.2",
    "@types/passport-github2": "1.2.5",
    "@types/pg": "8.6.1",
    "@types/pino-multi-stream": "3.1.2",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "pino-pretty": "7.2.0",
    "ts-jest": "27.0.7",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.3.5"
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-server.xml"
  }
}
