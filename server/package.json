{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "tsnd --ignore-watch node_modules --inspect --notify false -r dotenv/config -r tsconfig-paths/register -- ./index.ts | pino-pretty -i time,hostname,pid,host,method,remoteAddress",
    "swagger": "swagger-jsdoc -d swaggerDef.js -o ./public/swagger.yml ./src/routes/**/*.ts ./src/routes/**.ts",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "typeorm": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js -f ./src/ormconfig.ts"
  },
  "dependencies": {
    "@koa/cors": "3.0.0",
    "@koa/router": "8.0.8",
    "@octokit/app": "12.0.5",
    "@octokit/rest": "18.12.0",
    "aws-sdk": "2.906.0",
    "axios": "0.21.2",
    "http-status-codes": "2.1.4",
    "ics": "2.16.0",
    "json2csv": "4.5.4",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-basic-auth": "4.0.0",
    "koa-bodyparser": "4.2.1",
    "koa-jwt": "3.6.0",
    "koa-passport": "4.1.3",
    "koa-session": "5.13.1",
    "koa-static": "5.0.0",
    "koa-typeorm-pagination": "1.0.1",
    "lodash": "4.17.21",
    "luxon": "1.28.0",
    "moment-timezone": "0.5.33",
    "node-cache": "5.1.2",
    "node-schedule": "1.3.3",
    "passport-github": "1.1.0",
    "pg": "8.5.1",
    "pino-cloudwatch": "0.7.0",
    "pino-multi-stream": "4.2.0",
    "reflect-metadata": "0.1.13",
    "tsconfig-paths": "3.9.0",
    "typeorm": "0.2.37"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/json2csv": "4.5.1",
    "@types/jsonwebtoken": "8.5.5",
    "@types/koa": "2.11.3",
    "@types/koa-basic-auth": "2.0.4",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-passport": "4.0.2",
    "@types/koa-session": "5.10.2",
    "@types/koa-static": "4.0.2",
    "@types/koa__cors": "2.2.3",
    "@types/koa__router": "8.0.2",
    "@types/lodash": "4.14.176",
    "@types/luxon": "1.27.1",
    "@types/node": "14.17.29",
    "@types/node-schedule": "1.3.2",
    "@types/passport-github": "1.1.6",
    "@types/pg": "7.14.11",
    "@types/pino-multi-stream": "3.1.2",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "jest": "27.2.2",
    "jest-junit": "12.3.0",
    "pino-pretty": "3.6.1",
    "ts-jest": "27.0.7",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5"
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-server.xml"
  }
}
