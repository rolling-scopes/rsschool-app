{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "tsnd --ignore-watch node_modules --inspect --notify false -r dotenv/config -r tsconfig-paths/register -- ./index.ts | pino-pretty -i time,hostname,pid,host,method,remoteAddress",
    "swagger": "swagger-jsdoc -d swaggerDef.js -o ./public/swagger.yml ./src/routes/**/*.ts ./src/routes/**.ts",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "typeorm": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js -f ./src/ormconfig.ts"
  },
  "dependencies": {
    "@koa/cors": "~3.0.0",
    "@koa/router": "~8.0.8",
    "@octokit/app": "4.2.1",
    "@octokit/rest": "18.0.6",
    "aws-sdk": "~2.906.0",
    "axios": "~0.21.1",
    "http-status-codes": "~2.1.4",
    "ics": "2.16.0",
    "json2csv": "~4.5.2",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-basic-auth": "~4.0.0",
    "koa-bodyparser": "~4.2.1",
    "koa-jwt": "~3.6.0",
    "koa-passport": "~4.1.3",
    "koa-session": "~5.13.1",
    "koa-static": "~5.0.0",
    "koa-typeorm-pagination": "~1.0.1",
    "koa2-swagger-ui": "~2.13.2",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "moment-timezone": "~0.5.28",
    "node-cache": "~5.0.2",
    "node-schedule": "~1.3.2",
    "passport-github": "~1.1.0",
    "pg": "8.5.1",
    "pino-cloudwatch": "^0.7.0",
    "pino-multi-stream": "~4.2.0",
    "reflect-metadata": "0.1.13",
    "swagger-jsdoc": "3.5.0",
    "typeorm": "0.2.37"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/json2csv": "^4.5.0",
    "@types/jsonwebtoken": "8.3.5",
    "@types/koa": "^2.11.3",
    "@types/koa-basic-auth": "2.0.3",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-passport": "^4.0.2",
