{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "tsnd --ignore-watch node_modules --inspect --notify false -r dotenv/config -- ./src/index.ts | pino-pretty -i time,hostname,pid,host,method,remoteAddress",
    "lint": "eslint src",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "typeorm": "npx ts-node -r dotenv/config ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npx ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate -d src/dataSource.ts"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.8.0",
    "@koa/cors": "3.0.0",
    "@koa/router": "8.0.8",
    "@vendia/serverless-express": "4.5.3",
    "aws-sdk": "2.1354.0",
    "axios": "1.5.0",
    "dotenv": "10.0.0",
    "http-status-codes": "2.1.4",
    "ics": "2.31.0",
    "json2csv": "5.0.6",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-basic-auth": "4.0.0",
    "koa-bodyparser": "4.2.1",
    "koa-jwt": "3.6.0",
    "koa-passport": "5.0.0",
    "koa-session": "5.13.1",
    "koa-static": "5.0.0",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "moment-timezone": "0.5.34",
    "node-cache": "5.1.2",
    "node-schedule": "2.0.0",
    "octokit": "1.7.1",
    "passport-github2": "0.1.12",
    "passport-jwt": "4.0.0",
    "pg": "8.11.3",
    "pino-multi-stream": "4.2.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.14"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "@types/json2csv": "5.0.3",
    "@types/jsonwebtoken": "8.5.8",
    "@types/koa": "2.11.3",
    "@types/koa__cors": "2.2.3",
    "@types/koa__router": "8.0.2",
    "@types/koa-basic-auth": "2.0.4",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-passport": "4.0.3",
    "@types/koa-session": "5.10.2",
    "@types/koa-static": "4.0.2",
    "@types/lodash": "4.14.178",
    "@types/luxon": "2.0.9",
    "@types/node": "16.11.25",
    "@types/node-schedule": "1.3.2",
    "@types/passport-github2": "1.2.5",
    "@types/pg": "8.6.4",
    "@types/pino-multi-stream": "3.1.2",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.48.0",
    "eslint-config-turbo": "1.10.13",
    "jest": "29.6.4",
    "jest-junit": "16.0.0",
    "pino-pretty": "7.5.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.5.5"
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-server.xml"
  }
}
