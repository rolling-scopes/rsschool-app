    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^2.2.2",
    "@types/koa__router": "^8.0.2",
    "@types/lodash": "^4.14.136",
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.26.3",
    "@types/node": "^12.12.34",
    "@types/node-schedule": "^1.3.0",
    "@types/passport-github": "~1.1.5",
    "@types/pg": "^7.14.3",
    "@types/pino-multi-stream": "^3.1.2",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "jest": "~26.6.3",
    "pino-pretty": "~3.6.1",
