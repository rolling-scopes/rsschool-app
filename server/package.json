{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "license": "Mozilla Public License 2.0",
  "scripts": {
    "build": "tsc",
    "start": "tsnd --ignore-watch node_modules --inspect --notify false -r dotenv/config -- ./src/index.ts | pino-pretty -i time,hostname,pid,host,method,remoteAddress",
    "lint": "eslint src",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "npm run migration",
    "migration": "typeorm-ts-node-commonjs migration:generate -d src/dataSource.ts"
  },
  "dependencies": {
    "@apalchys/pino-cloudwatch": "0.9.0",
    "@koa/cors": "3.0.0",
    "@koa/router": "8.0.8",
    "@vendia/serverless-express": "4.10.4",
    "http-status-codes": "2.1.4",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-basic-auth": "4.0.0",
    "koa-bodyparser": "4.2.1",
    "koa-jwt": "3.6.0",
    "koa-static": "5.0.0",
    "luxon": "2.3.0",
    "moment-timezone": "0.5.34",
    "node-schedule": "2.0.0",
    "octokit": "1.7.1",
    "pg": "8.11.3",
    "pino-multi-stream": "4.2.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/json2csv": "5.0.7",
    "@types/jsonwebtoken": "8.5.8",
    "@types/koa": "2.11.3",
    "@types/koa__cors": "2.2.3",
    "@types/koa__router": "8.0.2",
    "@types/koa-basic-auth": "2.0.4",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-static": "4.0.2",
    "@types/luxon": "2.0.9",
    "@types/node-schedule": "1.3.2",
    "@types/passport-github2": "1.2.5",
    "@types/pg": "8.6.4",
    "@types/pino-multi-stream": "3.1.2",
    "cross-env": "7.0.3",
    "jest-junit": "16.0.0",
    "pino-pretty": "7.5.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0"
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-server.xml"
  }
}
