{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src",
    "declaration": false,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "lib": ["dom", "es2015", "es2016", "es2017"]
  },
  "include": ["next-env.d.ts", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
