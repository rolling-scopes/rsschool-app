{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build:lambda": "tsc --skipLibCheck --module commonjs --target es2018 --outDir dist lambda/app.ts",
    "ci:build": "tsc",
    "prod": "next start -p 8080",
    "lint": "eslint src",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/charts": "1.2.14",
    "@apalchys/next-aws-lambda": "3.6.7",
    "antd": "4.19.1",
    "axios": "0.26.0",
    "cookie": "0.4.2",
    "csvtojson": "2.0.10",
    "lodash": "4.17.21",
    "mobile-device-detect": "0.4.3",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-masonry-css": "1.0.16",
    "react-use": "17.3.2",
    "styled-jsx": "5.0.0",
    "use-places-autocomplete": "1.10.0"
  },
  "devDependencies": {
    "@playwright/test": "1.19.2",
    "@types/aws-lambda": "8.10.92",
    "@types/cookie": "0.4.1",
    "@types/csvtojson": "2.0.0",
    "@types/enzyme": "3.10.11",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.25",
    "@types/react": "17.0.39",
    "@types/react-color": "2.17.6",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "dotenv": "16.0.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.9.0",
    "jest": "27.5.1",
    "jest-junit": "13.0.0",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "nextBundleAnalysis": {
    "budget": 512000,
    "budgetPercentIncreaseRed": 5,
    "showDetails": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-client.xml"
  }
}
