{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build:lambda": "tsc --skipLibCheck --module commonjs --target es2018 --outDir dist lambda/app.ts",
    "ci:build": "tsc",
    "prod": "next start -p 8080",
    "lint": "eslint src",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/charts": "1.2.14",
    "@apalchys/next-aws-lambda": "3.6.7",
    "antd": "4.21.6",
    "axios": "0.27.2",
    "cookie": "0.5.0",
    "csvtojson": "2.0.10",
    "lodash": "4.17.21",
    "mobile-device-detect": "0.4.3",
    "moment": "2.29.4",
    "moment-timezone": "0.5.34",
    "next": "12.1.1",
    "react": "17.0.2",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-markdown": "6.0.2",
    "react-masonry-css": "1.0.16",
    "react-use": "17.4.0",
    "remark-gfm": "1.0.0",
    "styled-jsx": "5.0.2",
    "use-places-autocomplete": "3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.25.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@types/aws-lambda": "8.10.102",
    "@types/cookie": "0.5.1",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/node": "16",
    "@types/react": "17.0.39",
    "@types/react-color": "2.17.6",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "dotenv": "16.0.1",
    "eslint": "8.22.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-junit": "14.0.0",
    "typescript": "4.7.4"
  },
  "nextBundleAnalysis": {
    "budget": 512000,
    "budgetPercentIncreaseRed": 5,
    "showDetails": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-client.xml"
  }
}
