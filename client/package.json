{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "license": "Mozilla Public License 2.0",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build:lambda": "tsc --esModuleInterop --skipLibCheck --module commonjs --target es2018 --outDir dist lambda/app.ts",
    "build:analyze": "ANALYZE=1 next build",
    "ci:build": "tsc",
    "prod": "next start -p 8080",
    "lint": "eslint src",
    "test": "jest --silent",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --silent",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/compatible": "5.1.2",
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/plots": "1.2.5",
    "ahooks": "3.8.4",
    "antd": "5.11.2",
    "cookie": "0.7.0",
    "csvtojson": "2.0.10",
    "next": "13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7",
    "react-masonry-css": "1.0.16",
    "react-quill": "2.0.0",
    "react-use": "17.4.0",
    "remark-gfm": "3.0.1",
    "serverless-http": "3.2.0",
    "styled-jsx": "5.1.2",
    "use-places-autocomplete": "4.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.4.3",
    "@playwright/test": "1.49.1",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/aws-lambda": "8.10.126",
    "@types/cookie": "0.5.4",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "eslint-plugin-testing-library": "7.1.1",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-mock-axios": "4.7.3",
    "mq-polyfill": "1.1.8",
    "nextjs-bundle-analysis": "0.4.0"
  },
  "nextBundleAnalysis": {
    "budget": 512000,
    "budgetPercentIncreaseRed": 5,
    "showDetails": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-client.xml"
  }
}
