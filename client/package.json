{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build:lambda": "tsc --esModuleInterop --skipLibCheck --module commonjs --target es2018 --outDir dist lambda/app.ts",
    "ci:build": "tsc",
    "prod": "next start -p 8080",
    "lint": "eslint src",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/plots": "1.2.2",
    "antd": "4.23.1",
    "axios": "0.27.2",
    "cookie": "0.5.0",
    "csvtojson": "2.0.10",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.37",
    "next": "12.3.0",
    "react": "17.0.2",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.3",
    "react-masonry-css": "1.0.16",
    "react-sortable-hoc": "^2.0.0",
    "react-use": "17.4.0",
    "remark-gfm": "3.0.1",
    "serverless-http": "3.0.2",
    "styled-jsx": "5.0.7",
    "use-places-autocomplete": "3.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.0",
    "@playwright/test": "1.25.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/aws-lambda": "8.10.103",
    "@types/cookie": "0.5.1",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.14.184",
    "@types/node": "16.11.52",
    "@types/react": "17.0.39",
    "@types/react-color": "2.17.6",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.4",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "jest-junit": "14.0.1",
    "jest-mock-axios": "^4.7.0-beta",
    "mq-polyfill": "^1.1.8",
    "typescript": "4.8.3"
  },
  "nextBundleAnalysis": {
    "budget": 512000,
    "budgetPercentIncreaseRed": 5,
    "showDetails": true
  },
  "jest-junit": {
    "outputDirectory": "../reports",
    "outputName": "jest-junit-client.xml"
  }
}
